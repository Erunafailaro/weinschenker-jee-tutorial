<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC
    "-//Puppy Crawl//DTD Check Configuration 1.2//EN"
    "http://www.puppycrawl.com/dtds/configuration_1_2.dtd">

<module name="Checker">
	<property name="localeCountry" value="DE" />
	<property name="localeLanguage" value="en" />
	<!-- property name="severity" value="warning"/-->
	<module name="TreeWalker">
		<property name="tabWidth" value="4" />
		<module name="FileContentsHolder" />

		<module name="EmptyBlock">
			<property name="tokens"
				value="LITERAL_DO, LITERAL_ELSE, LITERAL_FINALLY, LITERAL_IF, LITERAL_FOR, LITERAL_TRY, LITERAL_WHILE, INSTANCE_INIT, STATIC_INIT" />
		</module>
		<module name="RightCurly" />
		<module name="FinalLocalVariable">
			<property name="severity" value="ignore" />
		</module>
		<module name="AvoidStarImport" />
		<module name="FinalParameters">
			<property name="severity" value="ignore" />
		</module>
		<module name="ModifierOrder" />
		<module name="RedundantModifier">
			<property name="severity" value="ignore" />
		</module>
		<module name="LineLength">
			<property name="max" value="300" /> <!-- Soll 130, defaults to to 80 -->
			<property name="ignorePattern" value="^ *\* \$Id" /> <!-- Reimer: kein $Id Zeilen -->
			<property name="ignorePattern" value="^ *\* " />
			<!-- property name="severity" value="ignore"/ -->
		</module>
		<module name="GenericWhitespace" />
		<module name="EmptyForInitializerPad" />
		<module name="EmptyForIteratorPad" />
		<module name="MethodParamPad" />
		<module name="NoWhitespaceAfter">
			<property name="tokens"
				value="ARRAY_INIT, BNOT, DEC, DOT, INC, UNARY_MINUS, UNARY_PLUS" />
		</module>
		<module name="NoWhitespaceBefore" />
		<module name="OperatorWrap">
			<!--
				property name="tokens" value="BAND, BOR, BSR, BXOR, COLON, DIV,
				EQUAL, GE, GT, LAND, LE, LITERAL_INSTANCEOF, LOR, LT, MINUS, MOD,
				NOT_EQUAL, PLUS, QUESTION, SL, SR, STAR"/
			-->
			<property name="tokens"
				value="BAND, BOR, BSR, BXOR, COLON, DIV, EQUAL, GE, GT, LE, LITERAL_INSTANCEOF, LT, MINUS, MOD, NOT_EQUAL, QUESTION, SL, SR, STAR" />
		</module>
		<module name="OperatorWrap">
			<property name="tokens" value="LAND, LOR, PLUS" />
			<property name="option" value="eol" />
		</module>
		<module name="ParenPad" />
		<module name="TypecastParenPad" />
		<!--module name="FileTabCharacter"/-->
		<module name="WhitespaceAfter">
			<property name="tokens" value="COMMA, SEMI" />
		</module>
		<module name="WhitespaceAround">
			<property name="tokens"
				value="BAND, BAND_ASSIGN, BOR, BOR_ASSIGN, BSR, BSR_ASSIGN, BXOR, BXOR_ASSIGN, COLON, DIV, DIV_ASSIGN, GE, LAND, LE, LITERAL_ASSERT, LITERAL_CATCH, LITERAL_DO, LITERAL_ELSE, LITERAL_FINALLY, LITERAL_FOR, LITERAL_IF, LITERAL_RETURN, LITERAL_SYNCHRONIZED, LITERAL_TRY, LITERAL_WHILE, LOR, MINUS_ASSIGN, MOD, MOD_ASSIGN, NOT_EQUAL, PLUS, PLUS_ASSIGN, QUESTION, SL, SLIST, SL_ASSIGN, SR, SR_ASSIGN, STAR, STAR_ASSIGN, TYPE_EXTENSION_AND" />
			<property name="severity" value="ignore" />
		</module>

		<module name="RegexpSinglelineJava">
			<!-- Trenne Negation -->
			<property name="format" value="![^\s=]" />
			<property name="ignoreComments" value="true" />
			<property name="message" value="whitespace required" />
			<property name="severity" value="ignore" />
		</module>
		<module name="RegexpSinglelineJava">
			<property name="ignoreComments" value="true" />
			<property name="format"
				value="System\.out\.print|System\.out\.println|System\.exit|e\.printStackTrace\(\)" />
			<property name="message" value="System.out used." />
		</module>
		<module name="RegexpSinglelineJava">
			<property name="format" value="[^.s]debug" />
			<property name="ignoreComments" value="true" />
			<property name="ignoreCase" value="false" />
			<property name="message" value="debug used." />
			<property name="severity" value="ignore" />
		</module>
		<module name="RegexpSinglelineJava">
			<property name="format" value="indexOf\s*\([^)]*\)\s*>\s*(0|1)" />
			<property name="ignoreCase" value="false" />
			<property name="message" value="incorrect indexOf usage." />
		</module>




		<!-- module name="GenericIllegalRegexp"-->
		<!-- \s matches whitespace character, $ matches end of line. -->
		<!--
			property name="format"
			value="(?:\x66\x75\x63\x6B)|(?:\x73\x68\x69\x74)|(?:\x63\x75\x6E\x74)"/>
			<property name="ignoreCase" value="true" /> <property name="message"
			value="Code contains inappropriate language"/> </module
		-->

		<!--
			http://checkstyle.sourceforge.net/config_coding.html#IllegalCatch
		-->

		<!-- module name="JavadocMethod"/ -->
		<module name="JavadocType">
			<property name="severity" value="ignore" />
		</module>
		<module name="JavadocVariable">
			<property name="severity" value="ignore" />
		</module>
		<module name="JavadocStyle">
			<property name="severity" value="ignore" />
		</module>
		<module name="ConstantName" />
		<module name="LocalFinalVariableName">
			<property name="severity" value="ignore" />
		</module>
		<module name="LocalVariableName" />
		<module name="MemberName">
			<property name="format" value="^[a-z][a-zA-Z0-9_]*$" />
		</module>
		<module name="MethodName">
			<property name="format" value="^[a-z][a-zA-Z0-9_]*$" />
		</module>
		<module name="PackageName" />
		<module name="ParameterName">
			<property name="format" value="^[a-z][a-zA-Z0-9_]*$" />
		</module>
		<module name="StaticVariableName" />
		<module name="TypeName" />
		<module name="IllegalImport" />
		<module name="RedundantImport" />
		<module name="UnusedImports" />
		<module name="MethodLength">
			<property name="max" value="300" /><!-- def 150 -->
		</module>
		<module name="ParameterNumber">
			<property name="max" value="12" /><!-- def 7 -->
		</module>
		<!--
			module name="TabCharacter"> <property name="severity"
			value="ignore"/> </module
		-->
		<module name="AvoidNestedBlocks">
			<property name="allowInSwitchCase" value="true" />
		</module>
		<module name="LeftCurly" />
		<module name="NeedBraces" />
		<module name="AvoidInlineConditionals">
			<property name="severity" value="ignore" />
		</module>
		<module name="DoubleCheckedLocking" />
		<module name="EmptyStatement" />
		<module name="EqualsHashCode" />
		<module name="HiddenField">
			<property name="severity" value="ignore" />
		</module>
		<module name="IllegalInstantiation">
			<property name="classes"
				value="java.lang.Boolean,java.lang.Double,java.lang.Integer" />
		</module>
		<module name="InnerAssignment" />
		<module name="MagicNumber">
			<property name="severity" value="ignore" />
		</module>
		<module name="MissingSwitchDefault" />
		<!-- module name="RedundantThrows"/ -->
		<module name="SimplifyBooleanExpression" />
		<module name="SimplifyBooleanReturn" />
		<module name="DesignForExtension">
			<property name="severity" value="ignore" />
		</module>
		<module name="FinalClass" />
		<module name="HideUtilityClassConstructor" />
		<module name="InterfaceIsType" />
		<module name="VisibilityModifier">
			<property name="protectedAllowed" value="true" />
		</module>
		<module name="ArrayTypeStyle" />

		<module name="TodoComment">
			<property name="format" value="FIXME|TODO" />
			<property name="severity" value="ignore" />
		</module>
		<module name="UpperEll" />
		<!-- not in Maven module name="WriteTag"/-->
		<module name="AbstractClassName">
			<property name="severity" value="ignore" />
		</module>
		<module name="ImportOrder">
			<property name="separated" value="true" />
			<property name="ordered" value="false" />
		</module>
		<!-- TODO was darf importiert werden module name="ImportControl"/-->
		<module name="AnonInnerLength" />
		<module name="ExecutableStatementCount">
			<!--
				property name="tokens" value="CTOR_DEF, METHOD_DEF, INSTANCE_INIT,
				STATIC_INIT"/
			-->
			<property name="max" value="275" /><!-- 30 default -->
		</module>
		<module name="CovariantEquals" />
		<module name="DefaultComesLast" />
		<module name="DeclarationOrder">
			<property name="severity" value="ignore" />
		</module>
		<module name="ExplicitInitialization" />
		<module name="FallThrough" />
		<module name="IllegalCatch">
			<property name="severity" value="ignore" />
		</module>
		<module name="IllegalThrows">
			<!-- reimer Throwable for Aspects -->
			<property name="illegalClassNames" value="java.lang.Error, java.lang.RuntimeException" />
		</module>
		<module name="IllegalType">
			<!-- complete disallowed -->
			<property name="illegalClassNames"
				value="java.lang.StringBuffer, java.util.LinkedHashMap, java.util.LinkedHashSet, java.util.TreeSet, java.util.TreeMap, java.util.Enumeration, java.util.Vector" />
			<property name="severity" value="ignore" />
		</module>
		<module name="IllegalType">
			<!-- als Variable ok -->
			<property name="tokens" value="METHOD_DEF,PARAMETER_DEF" />
			<property name="illegalClassNames"
				value="java.util.GregorianCalendar, java.util.Hashtable, java.util.HashSet, java.util.HashMap, java.util.ArrayList, java.util.LinkedList" />
			<property name="severity" value="ignore" />
		</module>
		<module name="JUnitTestCase" />
		<module name="MissingCtor">
			<property name="severity" value="ignore" />
		</module>
		<module name="ModifiedControlVariable" />
		<module name="MultipleStringLiterals">
			<property name="severity" value="ignore" />
		</module>
		<module name="MultipleVariableDeclarations" />
		<module name="NestedIfDepth">
			<property name="max" value="3" />
		</module>
		<module name="NestedTryDepth" />
		<module name="PackageDeclaration" />
		<module name="ParameterAssignment" />
		<module name="RequireThis">
			<property name="checkMethods" value="false" />
			<property name="severity" value="ignore" />
		</module>
		<module name="ReturnCount">
			<property name="max" value="4" />		<!-- 2 default -->
			<property name="format" value="^$" />
		</module>
		<module name="StringLiteralEquality" />
		<module name="SuperClone" />
		<module name="SuperFinalize" />
		<module name="ArrayTrailingComma" />
		<module name="UnnecessaryParentheses" />
		<module name="MutableException" />
		<module name="ThrowsCount">
			<property name="max" value="2" />
		</module>
		<module name="BooleanExpressionComplexity">
			<property name="max" value="10" /><!-- 3 default -->
		</module>
		<module name="ClassDataAbstractionCoupling">
			<property name="max" value="65" /><!-- 7 default -->
		</module>
		<module name="ClassFanOutComplexity">
			<property name="max" value="40" /><!-- 20 default -->
		</module>
		<module name="CyclomaticComplexity">
			<property name="max" value="80" /><!-- 10 default -->
		</module>
		<module name="JavaNCSS">
			<property name="methodMaximum" value="300" /><!-- 50 def -->
			<property name="classMaximum" value="1500" />
			<!--
				1500 def
			-->
			<property name="fileMaximum" value="2000" />
			<!--
				2000 def
			-->
		</module>
		<module name="NPathComplexity">
			<property name="max" value="20000" /><!-- 200 default -->
			<property name="severity" value="ignore" />
		</module>
		<module name="DescendantToken" />
		<module name="Indentation">
			<property name="severity" value="ignore" />
		</module>
		<module name="TrailingComment">
			<property name="severity" value="ignore" />
		</module>
		<module name="UncommentedMain">
			<property name="excludedClasses" value="Main$" />
		</module>
		<module name="IllegalToken">
			<property name="tokens" value="LITERAL_NATIVE" />
			<!-- LITERAL_SWITCH, POST_INC, POST_DEC -->
		</module>
		<module name="IllegalToken">
			<property name="severity" value="ignore" />
			<!-- LITERAL_SWITCH, POST_INC, POST_DEC -->
		</module>
		<module name="IllegalTokenText">
			<property name="tokens" value="NUM_INT,NUM_LONG" />
			<property name="format" value="^0[^lx]" />
			<property name="ignoreCase" value="true" />
		</module>



		<!-- 5.0 -->
		<module name="AnnotationUseStyle" />
		<module name="MissingDeprecated" />
		<module name="MissingOverride" />
		<module name="PackageAnnotation" />
		<module name="SuppressWarnings" />
		<module name="OuterTypeNumber" />
		<module name="NoFinalizer" />
		<module name="EqualsAvoidNull" />
		<module name="AvoidStaticImport">
			<property name="severity" value="ignore" />
		</module>
		<module name="NoClone" />

	</module>
	<module name="RegexpSingleline">
		<property name="format" value="[^*][ \t]+$" />
		<property name="message" value="Trailing whitespace" />
		<property name="severity" value="ignore" />
	</module>
	<module name="RegexpSingleline">
		<property name="format" value="^( [^*]|\t+ +[^*])" />
		<property name="message" value="beginning spaces" />
		<property name="severity" value="ignore" />
	</module>
	<module name="NewlineAtEndOfFile">
		<!--  xdoclet erzeugt unix -->
		<property name="severity" value="ignore" />
	</module>
	<module name="Translation" />
	<module name="StrictDuplicateCode">
		<property name="min" value="35" /><!-- 12 default-->
		<property name="severity" value="ignore" />
	</module>
	<module name="SeverityMatchFilter" />
	<module name="SuppressionFilter" />
	<module name="SuppressionCommentFilter" />
	<module name="SuppressWithNearbyCommentFilter" />
</module>
