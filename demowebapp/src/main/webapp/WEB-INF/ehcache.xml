<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">

	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="500" eternal="true"
		overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />


	<cache name="getCharacters" maxElementsInMemory="50" eternal="true"
		overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />

	<!-- cache name="org.weinschenker.demowebapp.persistence.entities.User"
		maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="300"
		timeToLiveSeconds="600" overflowToDisk="true" / -->

	<cache name="org.hibernate.cache.StandardQueryCache"
		maxElementsInMemory="5" eternal="false" timeToLiveSeconds="120"
		overflowToDisk="true" />
</ehcache>